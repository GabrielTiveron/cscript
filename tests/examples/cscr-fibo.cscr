// Série de Fibonacci
fibo(n){
  fibo-tco(n, 0, 1, [])
}

// Implementação com eliminação de recursão de cauda
fibo-tco(n, x, y, lst){
  if(n == 0){
    lst
  }
  else{
    x_ = y;
    y_ = x + y;
    lst_ = lst append(list x_);
    fibo-tco(n-1, x_, y_, lst_)
  }
}

// Imprime o resultado
printf(fibo(10)
