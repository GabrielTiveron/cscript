// Série de Fibonacci
func fibo(n){
  return fibo-tco(n, 0, 1, [])
}

// Implementação com eliminação de recursão de cauda
func fibo-tco(n, x, y, lst){
  if(n == 0){
    return lst
  }
  else{
    x_ = y;
    y_ = x + y;
    lst_ = lst append(list x_);
    return fibo-tco(n-1, x_, y_, lst_)
  }
}

// Imprime o resultado
printf(fibo(10)
